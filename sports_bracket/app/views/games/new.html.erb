 <%= f.label :scoreteam2 %>
 <%= f.text_field :scoreteam2 %>
<br>
 <%= f.label :teamwon %>
 <%= f.text_field :teamwon %>
<br>
 <%= f.submit "Submit" %>
<% end %>
