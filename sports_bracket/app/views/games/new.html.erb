<h2>Add New Game Score</h2>

<%= form_for [@game] do |f| %>
 <%= f.label :team1 %>
 <%= f.text_field :team1 %>
<br>
 <%= f.label :team2 %>
 <%= f.text_field :team2 %>
<br>
 <%= f.label :scoreteam1%>
 <%= f.text_field :scoreteam1 %>
